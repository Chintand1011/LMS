@model IEnumerable<OPBids.Entities.View.DTS.RequestBarcodeVM>
@Scripts.Render("~/bundles/dtsRequestBarcodeScript")
<div class="p-view-header">
    <h2 id="headerTitle">Request for Barcode</h2>
</div>
@{Html.RenderPartial("~/Views/DTS/BarcodeFilter.cshtml", new OPBids.Entities.View.DTS.DocumentSearchVM());}
<div id="addRemoveToolbar" class="col-md-5" style="padding:10px;">
    <a id="btnRequestBarcode" class="toolbar-add" href="#">
        <img src="~/Content/images/icon-add.png" />
        <span style="color: #4047cc;">Create Request for Barcode</span>
    </a>
</div>
<div id="pageContainerDocument" style="display:inline-block;float:right;padding-top:15px;">Page&nbsp;<select class="paging"></select>&nbsp;&nbsp;of&nbsp;<span class="pageTotal"></span></div>
@{Html.RenderPartial("~/Views/DTS/BarcodeList.cshtml", new List<OPBids.Entities.View.DTS.RequestBarcodeVM>());}
<span id="dataEntry">
    <input type="hidden" id="u_requested_quantity" />
    <input type="hidden" id="u_printed_quantity" />
    <input type="hidden" id="u_stat" />
    <input type="hidden" id="u_remarks" />
</span>