<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title - OPBids</title>
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/commonCss")
    <script type="text/javascript">
        FileUploader.UploadUrl = "@OPBids.Common.Constant.AppSettings.UploadFileUrl"
    </script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="../../content/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="../../content/Ionicons/css/ionicons.min.css">
    <!-- Date Picker -->
    <link rel="stylesheet" href="../../content/bootstrap-datepicker.min.css">
    
    <link rel="stylesheet" href="../../content/AdminLTE.css">
    <link rel="stylesheet" href="../../content/site.css"> @*included in bundle.config*@

    <link rel="stylesheet" href="../../content/bootstrap-select.min.css">


    @RenderSection("mycss",false)

    <link href="~/Content/FileUpload.css" rel="stylesheet" />




</head>
<body class="hold-transition sidebar-mini">

    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            @*<a href="index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>LT</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Admin</b>LTE</span>
            </a>*@



            <!-- Header Navbar: style can be found in header.less -->

        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                @Html.Partial("_LeftNavPartial")
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <header class="main-header">
                @Html.Action("TopBarPartial", "Shared")
            </header>
            <!-- Content Header (Page header) -->
            <!-- Main content -->
            <div class="body-content">
                <div>
                    @RenderBody()
                </div>


            </div>

            <!-- /.content -->


        </div>
        @*@Html.Partial("_SettingsRightSideBar")*@
       
        <div class="control-sidebar-bg"></div>
    </div>
    <div class="waitWindow"><div class="waitIcon"></div><div class="waitProgress"></div></div>

    <script src="~/Scripts/Site/jquery-ui.min.js"></script>
    @*<script>$.widget.bridge('uibutton', $.ui.button);</script>*@
    <script src="~/Scripts/Site/bootstrap.min.js"></script>
    <script src="~/Scripts/Site/bootstrap-select.min.js"></script>

    <!-- datepicker -->
    <script src="~/Scripts/Site/bootstrap-datepicker.min.js"></script>


    <script src="~/Scripts/Site/adminlte.js"></script>

    @Scripts.Render("~/bundles/appscripts")

    @RenderSection("scripts", required: false)






    <script>

        $().ready(function () {

            

        });

    </script>




</body>
</html>
