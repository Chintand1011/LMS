@model IEnumerable<OPBids.Entities.View.Shared.ActivityLogModel>

@{
    ViewBag.Title = "My Activity Log";
}

<h2>Activity Log</h2>

<table class="table table-striped table-sm">
    <thead>
        <tr>
            <th>Module</th>
            <th>Date Time</th>
            <th>IP Address</th>
            <th>User Name</th>
            <th>Full Name</th>
            <th>Activity</th>
            <th>Action</th>
            <th>Type</th>
        </tr>

    </thead>
    <tbody>

        @foreach (var itm in Model)
        {
            <tr>
                <td>@itm.Module</td>
                <td>@itm.DateTime</td>
                <td>@itm.IPAddress</td>
                <td>@itm.UserName</td>
                <td>@itm.FullName</td>
                <td>@itm.Activities</td>
                <td>@itm.Action</td>
                <td>@itm.Type</td>
            </tr>
        }

